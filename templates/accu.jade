extends _framework

block head

block body
  != page.html
  each post in widx.embed(page.parent)
    if (post != page)
      .article-wrap
        .article.embed-article
          h2.title
            a(href=post.url)
              = post.title
          span.text
            = widx.postprocess(post.html)

          mixin metadata(post.metadata)
